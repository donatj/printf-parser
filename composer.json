{
	"name": "donatj/printf-parser",
	"description": "PHP printf-syntax compatible printf string parser. Parses printf strings into a stream of lexemes.",
	"type": "library",
	"require-dev": {
		"donatj/drop": "*",
		"phpunit/phpunit": "~8 | ~9",
		"corpus/coding-standard": "^0.6.0",
		"squizlabs/php_codesniffer": "^3.10",
		"friendsofphp/php-cs-fixer": "^3.4",
		"phpstan/phpstan": "^1.11"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Jesse Donat",
			"email": "donatj@gmail.com"
		}
	],
	"require": {
		"php": ">=7.2"
	},
	"autoload": {
		"psr-4": {
			"donatj\\Printf\\": "src/"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
